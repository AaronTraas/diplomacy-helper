{
	"name": "Diplomacy Helper",
	"version": "0.1.0",
	"description": "Helper for playdiplomacy.com",
	"permissions": ["activeTab", "declarativeContent"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "images/icon.png"
		}
	},
	"icons": {
		"128": "images/icon.png"
	},
	"manifest_version": 2
}
